{% extends "NetworkUserBundle:EditProfile:edit.base.html.twig" %}
{% trans_default_domain 'FOSUserBundle' %}
{% block edit_content %}
    <div>
        <div>
            <form action="{{ path('fos_user_change_email') }}" {{ form_enctype(form) }} method="POST" class="fos_user_change_email">
                {{ form_widget(form) }}
                <div>
                    <input class="blue_button" type="submit" value="{{ 'change_email.submit'|trans }}" />
                </div>
            </form>
        </div>
    </div>
{% endblock %}
